{
  "name": "chatty-backend",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/app.ts | ./node_modules/.bin/bunyan",
    "start-db": "brew services start mongodb-community@6.0",
    "stop-db": "brew services stop mongodb-community@6.0",
    "check-db": "brew services list",
    "start-redis": "brew services start redis",
    "stop-redis": "brew services stop redis",
    "check-redis": "brew services info redis",
    "test-redis": "redis-cli",
    "run-redis": "redis-server",
    "redis-cmd": "redis-commander --port 3001",
    "lint:check": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}' --fix",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "nodemon ./build/app.js | ./node_modules/.bin/bunyan",
    "test": "jest --watch --coverage=true --maxWorkers=1 --forceExit --detectOpenHandles",
    "test:ci": "jest --coverage=true --watchAll=false"
  },
  "dependencies": {
    "@bull-board/express": "^4.9.0",
    "@bull-board/ui": "^4.9.0",
    "@sendgrid/mail": "^7.7.0",
    "@socket.io/redis-adapter": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.2",
    "bullmq": "^3.5.8",
    "bunyan": "^1.8.15",
    "cloudinary": "^1.32.0",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "ip": "^1.1.8",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^6.7.4",
    "nodemailer": "^6.8.0",
    "redis": "^4.5.1",
    "socket.io": "^4.5.4",
    "ttypescript": "^1.5.13",
    "typescript": "^4.9.3",
    "typescript-transform-paths": "^3.4.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/bunyan": "^1.8.8",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/hpp": "^0.2.2",
    "@types/ip": "^1.1.0",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/nodemailer": "^6.4.7",
    "@types/redis-info": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.1",
    "tsconfig-paths": "^4.1.0"
  }
}
